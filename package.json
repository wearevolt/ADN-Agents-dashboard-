{
  "name": "nextjs-agent-chat",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080 -H dev.agents.moneyball3.com",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,css,scss,md,mdx,json,yml,yaml}\" --cache",
